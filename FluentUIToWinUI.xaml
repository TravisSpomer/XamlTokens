<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	
	<Style TargetType="TextBlock">
		<Setter Property="FontFamily" Value="{ThemeResource BodyFontFamily}" />
		<Setter Property="FontWeight" Value="{ThemeResource BodyFontWeight}" />
		<Setter Property="FontSize" Value="{ThemeResource BodyFontSize}" />
		<Setter Property="LineHeight" Value="{ThemeResource BodyFontLineHeight}" />
		<Setter Property="TextWrapping" Value="WrapWholeWords" />
		<Setter Property="Margin" Value="0, 8, 0, 8" />
	</Style>

	<Style x:Key="HeaderTextBlockStyle" TargetType="TextBlock">
		<Setter Property="FontFamily" Value="{ThemeResource HeaderFontFamily}" />
		<Setter Property="FontWeight" Value="{ThemeResource HeaderFontWeight}" />
		<Setter Property="FontSize" Value="{ThemeResource HeaderFontSize}" />
		<Setter Property="LineHeight" Value="{ThemeResource HeaderFontLineHeight}" />
		<Setter Property="Margin" Value="0, 16, 0, 16" />
	</Style>

	<Style x:Key="SubheaderTextBlockStyle" TargetType="TextBlock">
		<Setter Property="FontFamily" Value="{ThemeResource SubheaderFontFamily}" />
		<Setter Property="FontWeight" Value="{ThemeResource SubheaderFontWeight}" />
		<Setter Property="FontSize" Value="{ThemeResource SubheaderFontSize}" />
		<Setter Property="LineHeight" Value="{ThemeResource SubheaderFontLineHeight}" />
		<Setter Property="Margin" Value="0, 12, 0, 12" />
	</Style>
			
	<!-- Button style-independent design tokens -->
	<StaticResource x:Key="ButtonPadding" ResourceKey="ButtonRootLayoutPadding" />
	<StaticResource x:Key="ButtonBorderThemeThickness" ResourceKey="ButtonRootStrokeWidth" />
	<StaticResource x:Key="ContentControlThemeFontFamily" ResourceKey="ButtonTextFontFamily" />
	<StaticResource x:Key="ContentControlThemeFontWeight" ResourceKey="ButtonTextFontWeight" />
	<StaticResource x:Key="ControlContentThemeFontSize" ResourceKey="ButtonTextFontSize" />
	<StaticResource x:Key="ControlContentThemeFontLineHeight" ResourceKey="ButtonTextFontLineHeight" />

	<!-- Button (neutral) design tokens-->
	<StaticResource x:Key="ButtonForeground" ResourceKey="ButtonTextFillColorRest" />
	<StaticResource x:Key="ButtonBackground" ResourceKey="ButtonRootFillColorRest" />
	<StaticResource x:Key="ButtonBorderBrush" ResourceKey="ButtonRootStrokeColorRest" />
	<StaticResource x:Key="ButtonForegroundPointerOver" ResourceKey="ButtonTextFillColorHover" />
	<StaticResource x:Key="ButtonBackgroundPointerOver" ResourceKey="ButtonRootFillColorHover" />
	<StaticResource x:Key="ButtonBorderBrushPointerOver" ResourceKey="ButtonRootStrokeColorHover" />
	<StaticResource x:Key="ButtonForegroundPressed" ResourceKey="ButtonTextFillColorPress" />
	<StaticResource x:Key="ButtonBackgroundPressed" ResourceKey="ButtonRootFillColorPress" />
	<StaticResource x:Key="ButtonBorderBrushPressed" ResourceKey="ButtonRootStrokeColorPress" />
	<StaticResource x:Key="ButtonForegroundDisabled" ResourceKey="ButtonTextFillColorDisabled" />
	<StaticResource x:Key="ButtonBackgroundDisabled" ResourceKey="ButtonRootFillColorDisabled" />
	<StaticResource x:Key="ButtonBorderBrushDisabled" ResourceKey="ButtonRootStrokeColorDisabled" />

	<!-- AccentButton design tokens -->
	<StaticResource x:Key="AccentButtonForeground" ResourceKey="AccentButtonTextFillColorRest" />
	<StaticResource x:Key="AccentButtonBackground" ResourceKey="AccentButtonRootFillColorRest" />
	<StaticResource x:Key="AccentButtonBorderBrush" ResourceKey="AccentButtonRootStrokeColorRest" />
	<StaticResource x:Key="AccentButtonForegroundPointerOver" ResourceKey="AccentButtonTextFillColorHover" />
	<StaticResource x:Key="AccentButtonBackgroundPointerOver" ResourceKey="AccentButtonRootFillColorHover" />
	<StaticResource x:Key="AccentButtonBorderBrushPointerOver" ResourceKey="AccentButtonRootStrokeColorHover" />
	<StaticResource x:Key="AccentButtonForegroundPressed" ResourceKey="AccentButtonTextFillColorPress" />
	<StaticResource x:Key="AccentButtonBackgroundPressed" ResourceKey="AccentButtonRootFillColorPress" />
	<StaticResource x:Key="AccentButtonBorderBrushPressed" ResourceKey="AccentButtonRootStrokeColorPress" />
	<StaticResource x:Key="AccentButtonForegroundDisabled" ResourceKey="AccentButtonTextFillColorDisabled" />
	<StaticResource x:Key="AccentButtonBackgroundDisabled" ResourceKey="AccentButtonRootFillColorDisabled" />
	<StaticResource x:Key="AccentButtonBorderBrushDisabled" ResourceKey="AccentButtonRootStrokeColorDisabled" />

	<!-- OutlineButton design tokens -->
	<StaticResource x:Key="OutlineButtonForeground" ResourceKey="OutlineButtonTextFillColorRest" />
	<StaticResource x:Key="OutlineButtonBackground" ResourceKey="OutlineButtonRootFillColorRest" />
	<StaticResource x:Key="OutlineButtonBorderBrush" ResourceKey="OutlineButtonRootStrokeColorRest" />
